//datatypes
// primitive datatype

let name="Arpit" //string type
let age=21; //number type (range 2 to power 53)
let  isLoggedIn= false; //boolean type
let state; //undefined type
let city=null // null type is object type, which may be language error.

/*
null is defined (empty) standalone value
undefined is value or type is not defined.
symbol is used for uniqueness
*/

